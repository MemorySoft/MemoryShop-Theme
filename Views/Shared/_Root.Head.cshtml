@using Nop.Core
@using Nop.Core.Infrastructure;
@{
    var displayMiniProfiler = CommonHelper.GetTrustLevel() >= AspNetHostingPermissionLevel.High &&
        EngineContext.Current.Resolve<Nop.Core.Domain.StoreInformationSettings>().DisplayMiniProfilerInPublicStore;

    //NopShop Scripts
    Html.AppendScriptParts("~/Themes/MemoryShop/Scripts/styleSheetToogle.js");
    Html.AppendScriptParts("~/Themes/MemoryShop/Scripts/jquery.slimscroll.min.js");
    Html.AppendScriptParts("~/Themes/MemoryShop/Scripts/html5shiv.js"); 
    Html.AppendScriptParts("~/Themes/MemoryShop/Scripts/main.js");
    Html.AppendScriptParts("~/Themes/MemoryShop/Scripts/jquery.prettyPhoto.js"); 
    Html.AppendScriptParts("~/Themes/MemoryShop/Scripts/price-range.js"); 
    Html.AppendScriptParts("~/Themes/MemoryShop/Scripts/jquery.scrollUp.min.js");
    Html.AppendScriptParts("~/Scripts/jquery-ui-1.10.3.custom.min.js");
    Html.AppendScriptParts("~/Themes/MemoryShop/Content/bootstrap-3.3.5/js/bootstrap.min.js");

    //resources
    Html.AppendScriptParts("~/Scripts/public.ajaxcart.js");
    Html.AppendScriptParts("~/Scripts/public.common.js");
    Html.AppendScriptParts("~/Scripts/jquery-migrate-1.2.1.min.js");
    Html.AppendScriptParts("~/Scripts/jquery.validate.unobtrusive.min.js");
    Html.AppendScriptParts("~/Scripts/jquery.validate.min.js");
    Html.AppendScriptParts("~/Scripts/jquery-1.10.2.min.js");    
}
<!DOCTYPE html>
<html @Html.Partial("LanguageAttributes")>
<head>
    <title>@Html.NopTitle(true)</title>
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8" />
    <meta name="description" content="@(Html.NopMetaDescription())" />
    <meta name="keywords" content="@(Html.NopMetaKeywords())" />
    <meta name="generator" content="nopCommerce" />
    @Html.NopHeadCustom()
    @*This is used so that themes can inject content into the header*@
    @Html.Partial("Head")
    @Html.Widget("head_html_tag")
    @Html.NopCssFiles(this.Url, ResourceLocation.Head)
    @Html.NopScripts(this.Url, ResourceLocation.Head)
    @Html.NopCanonicalUrls()
    @Html.Action("RssHeaderLink", "News")
    @Html.Action("RssHeaderLink", "Blog")
    @*Favicon - upload favicon.ico file to the root directory*@
    @Html.Action("Favicon", "Common")
    @if (displayMiniProfiler)
    {
        @StackExchange.Profiling.MiniProfiler.RenderIncludes();
    }
    
    <!--[if lt IE 9]>
      <p class="browsehappy">
        Estas usando una <strong>versión obsoleta de Internet Explorer</strong> que ya no tiene soporte de Microsoft y puede dañar la integridad de tu ordenador. Por favor <a href="http://browsehappy.com/">actualízate</a> para mejorar tu experiencia de navegación.
      </p>
    <![endif]-->
    
</head>
    <body>
        @RenderBody()
        @Html.NopCssFiles(this.Url, ResourceLocation.Foot)
        @Html.NopScripts(this.Url, ResourceLocation.Foot)
    </body>
</html>
